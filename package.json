{
  "name": "smirkle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "cap:init": "npx cap init smirkle com.example.smirkle --web-dir=dist",
    "cap:add:android": "npx cap add android",
    "cap:copy": "npx cap copy",
    "cap:sync": "npx cap sync",
    "cap:update": "npx cap update android",
    "cap:open:android": "npx cap open android",
    "build:android": "npm run build && npx cap copy android"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^6.0.0",
    "vite-plugin-pwa": "^0.21.1"
  },
  "dependencies": {
    "@capacitor/core": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tensorflow/tfjs": "~4.17.0",
    "@vladmandic/face-api": "^1.7.12",
    "firebase": "^12.9.0",
    "lucide-react": "^0.563.0",
    "react-webcam": "^7.2.0"
  }
}
